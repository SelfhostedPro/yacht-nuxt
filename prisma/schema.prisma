// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../config/sqlite.db"
}

model User {
  id             Int    @id @default(autoincrement())
  name           String @unique
  hashedPassword String
  role           String
}
